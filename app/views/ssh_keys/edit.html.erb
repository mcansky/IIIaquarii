<% content_for :head do %>
  <title><%= t(:edit, :scope => :sshkeys) %> <%= @key.name %> - <%= Settings.application.name %></title>
<% end %>

<div id="sshkeys">
  <h2><%= t(:edit, :scope => :sshkeys) %></h2>

  <%= render "form", :key => @key %>

  <%= link_to t(:list, :scope => :sshkeys), ssh_keys_path %> | <%= link_to t(:cancel, :scope => :links), @key %>
</div>
